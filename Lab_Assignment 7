#include <stdio.h>
#include <stdlib.h>

int arr[] = {97, 16, 45, 63, 13, 22, 7, 58, 72};
int i = 0;
int j = 0;
int size = sizeof(arr) / sizeof(arr[0]);
int temp = 0;

while(j != size){

    int swap_count = 0;

     for(i=0+j; i+j<size; i++){

        if(arr[i+j]>arr[i+1+j]){

            temp = arr[i+j];
            arr[i+j] = arr[i+1+j];
            arr[i+1+j] = temp;

            swap_count+=1;
        }
    
    }

    
    printf("%d",swap_count);

j++;
}